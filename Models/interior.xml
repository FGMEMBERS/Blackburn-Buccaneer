			<z-m>0.87</z-m>
			<roll-deg>0</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>

	<model>
		<path>Aircraft/Blackburn-Buccaneer/Instruments/blowing_press.xml</path>
