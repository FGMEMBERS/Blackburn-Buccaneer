			<y-m>0.15</y-m>
			<z-m>0.97</z-m>
			<roll-deg>-12.5</roll-deg>
			<pitch-deg>-10</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>

	<!-- *********** Port Coaming Panel ********** -->
	<model>
		<path>Aircraft/Blackburn-Buccaneer/Instruments/port_coaming_panel.xml</path>
		<offsets>
			<x-m>3.36</x-m>
			<y-m>-0.35</y-m>
			<z-m>0.81</z-m>
			<heading-deg>0</heading-deg>
		</offsets>
			<overlay>
		 <animation n="101">
    <type>scale</type>
    <property>constants/zero</property>
    <x-offset>1.5</x-offset>
    <y-offset>1.5</y-offset>
    <z-offset>1.5</z-offset>
   </animation>
   </overlay>
	
	</model>

	<model>
		<path>Aircraft/Blackburn-Buccaneer/Instruments/stbd_coaming_panel.xml</path>
