			<y-m>0.29</y-m>
			<z-m>0.85</z-m>
			<heading-deg>-15</heading-deg>
		</offsets>
		<overlay>
		 <animation n="101">
    <type>scale</type>
    <property>constants/zero</property>
    <x-offset>1.3</x-offset>
    <y-offset>1.3</y-offset>
    <z-offset>1.3</z-offset>
   </animation>
   </overlay>
	</model>

