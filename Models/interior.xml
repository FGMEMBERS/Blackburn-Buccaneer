	<!-- hide all of the model not visible from the cockpit -->
	<!-- order objects -->

	<animation>
		<type>select</type>
		<object-name>Radar-Panel</object-name>
		<condition>
			<and>
				<property>sim/current-view/internal</property>
				<not>
					<equals>
						<property>sim/current-view/name</property>
						<value>Model Cockpit View</value>
					</equals>
				</not>
			</and>
		</condition>
	</animation>


