<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
<model>
<path>cockpit.ac</path>
<offsets>
			<x-m>9.16</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.02</z-m>
		</offsets>
</model>

	<!-- ***************** Cockpit *************************** -->
	<rear-panel>
		<offsets>
			<x-m>-4.6</x-m>
			<y-m>-0.4</y-m>
			<z-m>0.65</z-m>
		</offsets>
	</rear-panel>

	<panel>
		<path>Aircraft/Blackburn-Buccaneer/Panels/radar-panel.xml</path>
		<name>Radar-Panel</name>
		<bottom-left>
			<x-m>4.6</x-m>
			<!--<x-m alias="../../../rear-panel/offsets/x-m"/>-->
			<y-m>-0.4</y-m>
			<!--<y-m alias="../../../rear-panel/offsets/y-m"/>-->
			<z-m>0.65</z-m>
			<!--<z-m alias="../../../rear-panel/offsets/z-m"/>-->
		</bottom-left>
		<bottom-right>
			<x-m>4.40</x-m>
			<!--<x-m alias="../../../rear-panel/offsets/x-m"/>-->
			<y-m>-0.15</y-m>
			<z-m>0.65</z-m>
			<!--<z-m alias="../../../rear-panel/offsets/z-m"/>-->
		</bottom-right>
		<top-left>
			<x-m>4.6</x-m>
			<!--<x-m alias="../../../rear-panel/offsets/x-m"/>-->
			<y-m>-0.4</y-m>
			<!--<y-m alias="../../../rear-panel/offsets/y-m"/>-->
			<z-m> 0.9</z-m>
		</top-left>
	</panel>

	<animation>
		<type>rotate</type>
		<object-name>Radar-Panel</object-name>
		<offset>45</offset>
		<center>
			<x-m>4.07546</x-m>
			<y-m>-0.0499988</y-m>
			<z-m>0.920013</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- **************** Instruments ********************* -->



	<!-- **************** Over Glare Shield********** -->
	<over-main-panel>
		<offsets>
			<x-m>3.36104</x-m>
		</offsets>
	</over-main-panel>

